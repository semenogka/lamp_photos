	// Открытие формы обратной связи для заказа товара
	$(document).ready(function($){
		$(".fancy_order_form").on('click', function(){
			var id = $(this).data('item');
			var count = $(this).parent().find('input.quantity').val();
			$.fancybox({
				href: '/produktsiya/form.php?form=1&id='+id+'&count='+count,
				margin: 0,
				padding: 20,
				maxWidth: '90%',
				maxHeight: '90%',
				autoScale: true,
				transitionIn: 'none',
				transitionOut: 'none',
