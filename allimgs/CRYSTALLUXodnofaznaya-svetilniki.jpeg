			<script>
				document.querySelector('.selected-option').addEventListener('click', e=>{
					let options = e.target.closest('.custom-select').querySelector('.options');
					options.classList.toggle('active');
					let selected = e.target.closest('.selected-option')

					selected.querySelector('i').classList.toggle('fa-angle-down');
					selected.querySelector('i').classList.toggle('fa-angle-up');
				});

				document.querySelectorAll('.custom-select .options > div').forEach(li => {
					li.addEventListener('click', e=>{
						let selected_option = e.target.closest('.custom-select').querySelector('.selected-option');
						let options_block = selected_option.nextElementSibling;

						let same = selected_option.querySelector('span').innerText == e.target.innerText;

						selected_option.querySelector('span').innerText = e.target.innerText;
						
						options_block.classList.toggle('active');

						selected_option.querySelector('i').classList.toggle('fa-angle-down');
						selected_option.querySelector('i').classList.toggle('fa-angle-up');

						if (!same)
						SortedBy(li.dataset.value);
					});
				});

				document.addEventListener('click', e=>{
					let select = document.querySelector('.custom-select');

					let options = select.querySelector('.options');
					if (!select.contains(e.target) && options.classList.contains('active'))
					{
						options.classList.remove('active');

						select.querySelector('i').classList.toggle('fa-angle-down');
						select.querySelector('i').classList.toggle('fa-angle-up');
					}
				});

				function SortedBy(sortedBy)
				{
					const url = new URL(window.location);

					url.searchParams.set('SORT_BY', sortedBy);

					document.location = url;
				}

			</script>
						


